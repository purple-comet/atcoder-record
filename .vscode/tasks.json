{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
  "tasks": [
    {
      "label": "AtCoder Test (PyPy)",
      "type": "shell",
      "command": "oj",
      "args": [
        "t",
        "-c",
        "pypy3 main.py"
      ],
      "options": {
      "cwd": "${fileDirname}",
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": true
      },
      "problemMatcher": []
    }
  ]
}